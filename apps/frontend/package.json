{
    "name": "@autoflow/frontend",
    "version": "1.0.0",
    "description": "AutoFlow Frontend Application",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 5",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:coverage": "vitest --coverage",
        "test:e2e": "playwright test",
        "test:e2e:ui": "playwright test --ui",
        "test:e2e:headed": "playwright test --headed",
        "test:all": "npm run test && npm run test:e2e"
    },
    "dependencies": {
        "framer-motion": "^10.12.6",
        "@hookform/resolvers": "^5.2.2",
        "@tanstack/react-query": "^5.90.1",
        "@tanstack/react-router": "^1.131.50",
        "axios": "^1.12.2",
        "clsx": "^2.1.1",
        "lucide-react": "^0.544.0",
        "material-symbols": "^0.36.0",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "react-hook-form": "^7.63.0",
        "react-hot-toast": "^2.6.0",
        "react-logos": "^0.1.0",
        "reactflow": "^11.11.4",
        "simple-icons": "^15.16.0",
        "tailwindcss": "^4.1.13",
        "zod": "^4.1.11",
        "zustand": "^5.0.8"
    },
    "devDependencies": {
        "@playwright/test": "^1.55.0",
        "@tailwindcss/postcss": "^4.1.13",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "@typescript-eslint/eslint-plugin": "^8.44.1",
        "@typescript-eslint/parser": "^8.44.1",
        "@vitejs/plugin-react": "^5.0.3",
        "autoprefixer": "^10.4.21",
        "eslint": "^9.36.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.21",
        "jsdom": "^27.0.0",
        "postcss": "^8.5.6",
        "typescript": "^5.9.2",
        "vite": "^7.1.7",
        "vitest": "^3.2.4"
    }
}