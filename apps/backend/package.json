{
    "name": "@autoflow/backend",
    "version": "1.0.0",
    "description": "AutoFlow Backend API",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "dev": "tsx watch src/index.ts",
        "start": "node dist/index.js",
        "lint": "eslint src/**/*.ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist",
        "db:generate": "drizzle-kit generate:pg",
        "db:migrate": "drizzle-kit push:pg",
        "db:studio": "drizzle-kit studio"
    },
    "dependencies": {
        "@fastify/cors": "^8.3.0",
        "@fastify/jwt": "^7.2.0",
        "@fastify/multipart": "^7.6.0",
        "@fastify/rate-limit": "^8.0.0",
        "@fastify/swagger": "^8.8.0",
        "@fastify/swagger-ui": "^1.9.3",
        "drizzle-orm": "^0.28.6",
        "fastify": "^4.21.0",
        "pg": "^8.11.2",
        "postgres": "^3.3.5",
        "redis": "^4.6.7",
        "zod": "^3.22.2",
        "openai": "^4.6.0",
        "axios": "^1.5.0",
        "bcrypt": "^5.1.1",
        "jsonwebtoken": "^9.0.2",
        "bull": "^4.11.3"
    },
    "devDependencies": {
        "@types/node": "^20.5.7",
        "@types/pg": "^8.10.2",
        "@types/bcrypt": "^5.0.0",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/jest": "^29.5.4",
        "drizzle-kit": "^0.19.13",
        "eslint": "^8.48.0",
        "jest": "^29.6.4",
        "tsx": "^3.12.7",
        "typescript": "^5.2.2",
        "ts-jest": "^29.1.1"
    }
}